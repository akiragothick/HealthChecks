{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "HealthChecks": [
    {
      "Name": "Cursos HTTP Check",
      "Base": "http://localhost:38573",
      "Tag": "Cursos",
      "Uris": [
        {
          "Name": "Application-check",
          "Uri": "/v1/application/healthy"
        }
      ]
    }
  ],
  "HealthDBChecks": [
    {
      "Name": "SQL DB Check",
      "ConnectionString": "Data Source={Source};Initial Catalog={Database};Integrated security = True;"
    }
  ],
  "HealthWSChecks": [
    {
      "Name": "displayname",
      "Service": "servicename"
    }
  ]
}
